<?xml version='1.0' encoding='utf-8'?>
<widget xmlns:android="http://schemas.android.com/apk/res/android" id="eu.imback.os.app" version="5.0.A" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>DBG ImB HTML App</name>
    <description>DBG ImBack HTML App</description>
    <author email="stefan@hegny.de">
        Stefan Hegny
    </author>
    <content src="http://192.168.0.72:8080/imbapp-dbg.html" />
    <!--content src="http://192.168.8.101:8080/imbapp-dbg.html" /-->
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <acces origin="http://192.168.0.72:8080" />
    <!--acces origin="http://192.168.8.101:8080" /-->
    <preference name="scheme" value="http" />
    <preference name="hostname" value="192.168.0.72:8080" />
    <!--preference name="hostname" value="192.168.8.101:8080" /-->
    <platform name="android">
    	<edit-config file="AndroidManifest.xml" mode="merge" target="/manifest/application">
            <application android:usesCleartextTraffic="true" />
            <!--application android:usesCleartextTraffic="true" android:usesLegacyExternalStorage="true"/-->
    	</edit-config>
    	<!--edit-config file="AndroidManifest.xml" mode="merge" target="/manifest">
	    <uses-permission android:name="android.Manifest.permission.WRITE_EXTERNAL_STORAGE" />
    	</edit-config-->
    </platform>
    <config-file target="AndroidManifest.xml" parent="/*">
    	<uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    	<uses-permission android:name="android.permission.MANAGE_MEDIA"/>
    </config-file>
    <icon src="res/logo.png"/>
    <!--preference name="android-targetSdkVersion" value="29"/>
    <preference name="android-targetSdkVersion" value="19"/-->
    <preference name="AndroidPersistentFileLocation" value="compatibility" />
    <preference name="AndroidXEnabled" value="true" />
</widget>
