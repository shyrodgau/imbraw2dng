<?xml version='1.0' encoding='utf-8'?>
<widget xmlns:android="http://schemas.android.com/apk/res/android" id="eu.imback.os.app" version="0.8.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>ImB HTML App</name>
    <description>ImBack HTML App</description>
    <author email="stefan@hegny.de">
        Stefan Hegny
    </author>
    <content src="http://192.168.1.254/imbapp.html" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <acces origin="http://192.168.1.254" />
    <preference name="scheme" value="http" />
    <preference name="hostname" value="192.168.1.254" />
    <platform name="android">
        <edit-config file="AndroidManifest.xml" mode="merge" target="/manifest/application">
            <application android:usesCleartextTraffic="true"/>
        </edit-config>
    </platform>
    <config-file target="AndroidManifest.xml" parent="/*">
        <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE"/>
        <uses-permission android:name="android.permission.MANAGE_MEDIA"/>
    </config-file>
    <icon src="res/logo.png"/>
    <preference name="AndroidXEnabled" value="true" />
    <preference name="AndroidPersistentFileLocation" value="Compatibility" />
</widget>
